{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*",
                "src/types/*"
            ]
        },
        "sourceMap": true,
        "lib": ["es2017"]
    },
    "files": [
        "server.ts",
        "chatserver.ts",
        "webscraper/main.ts"
    ],
    "include": [
        "*"
    ],
    "exclude": [
        "node_modules"
    ]
}